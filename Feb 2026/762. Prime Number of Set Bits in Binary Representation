class Solution {
public:
    vector<int> detob(int n){
        vector<int> bi;
        while(n>=1){
            if(n%2 == 0){
                bi.push_back(0);
            }else{
                bi.push_back(1);
            }
            n = n/2;
        }
        if(n==0) bi.push_back(0);
        if(n==1) bi.push_back(1);
        reverse(bi.begin(),bi.end());
        return bi;
    }
    int isprime(int n){
       if(n<=1) return false;
       int cnt = 0;
       for(int i = 1;i*i<=n;i++){
        if(n%i ==0){
            cnt++;
        
        if(n/i != i) cnt++;
        if(cnt>2) return false;
       }
       }
       return (cnt==2);
    }
    int countPrimeSetBits(int left, int right) {
        int ans = 0;
        for(int i = left;i<=right;i++){
            vector vals = detob(i);
            int count = 0;
            for(int i = 0;i<vals.size();i++){
                if(vals[i] == 1) count++;
            }
            if(isprime(count)) ans++;
        }
        return ans;
    }
};
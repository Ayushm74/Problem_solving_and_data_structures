class Solution {
public:
    bool hasAllCodes(string s, int k) {
        set<string> vals;
                if(k > s.size()) return false; 
        for(int i = 0;i<s.size()-k+1;i++){
            string s2 = "";
            for(int j = i; j < i+k && j < s.size(); j++){
                s2 += s[j];
            }
            vals.insert(s2);
        }
        long long v2 = pow(2,k);
        if(vals.size()==v2) return true;
        return false;
    }
};